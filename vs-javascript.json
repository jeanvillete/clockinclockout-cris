			"\t",
			"\t${Controller}Controller.$inject = ['${dependency1}'];",
			"\t/* @ngInject */",
			"\tfunction ${Controller}Controller(${dependency1}) {",
			"\t\tvar vm = this;",
			"\t\t$0",
			"",
			"\t\tactivate();",
			"",
			"\t\t////////////////",
			"",
			"\t\tfunction activate() { }",
			"\t}",
			"})();"
